<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/my_navigation"
    app:startDestination="@id/showFragment">

    <fragment
        android:id="@+id/showFragment"
        android:name="com.example.roompractic.ShowFragment"
        android:label="List Users">
        <action
            android:id="@+id/action_showFragment_to_editFragment"
            app:destination="@id/editFragment" />
    </fragment>
    <fragment
        android:id="@+id/editFragment"
        android:name="com.example.roompractic.EditFragment"
        android:label="Add User">
        <action
            android:id="@+id/action_editFragment_to_showFragment"
            app:destination="@id/showFragment" />
    </fragment>
</navigation>